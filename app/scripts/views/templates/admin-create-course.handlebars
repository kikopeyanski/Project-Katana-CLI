<div class="container">
  <div class="row">
    <form role="form" class="col-md-9 go-right">
      <h2>Crate Course</h2>
      <div class="form-group">
        <input id="name" name="name" type="text" class="form-control" required>
        <label for="name">Course name</label>
      </div>
      <div class="row">
        <div class="form-group col-md-6">
          <input id="start-date" name="start-date" type="date" class="form-control" required>
          <label for="start-date">Course Start Date</label>
        </div>
        <div class="form-group col-md-6">
          <input id="end-date" name="edn-date" type="date" class="form-control" required>
          <label for="end-date">Course End Date</label>
        </div>
        <div class="row">
          <input type="hidden" name="count" value="1"/>
          <div class="control-group" id="fields">
            <label class="control-label" for="field1">Nice Multiple Form Fields</label>
            <div class="controls" id="profs">
              <form class="input-append">
                <div id="field">
                  <input autocomplete="off" class="input" id="field1" name="prof1" type="text"
                         placeholder="Type something" data-items="8"/>
                  <button id="b1" class="btn add-more" type="button">+</button>
                </div>
              </form>
              <br>
              <small>Press + to add another form field :)</small>
            </div>
          </div>
        </div>
      </div>
      <button class="btn btn-success" id="course-create-btn">Create Course</button>
    </form>
  </div>
</div>
