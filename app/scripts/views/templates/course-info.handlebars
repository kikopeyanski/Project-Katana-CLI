<div>
  <div class="col-md-10">
    <h1> Course Name: {{this.course.name}}</h1>
  </div>
  <div class="col-md-6">
    Course start date:
    {{course.startDate}}
  </div>
  <div class="col-md-6">
    Course end date:
    {{course.endDate}}
  </div>
  {{#each course.homework}}
    <div class="col-md-4"><h2>
      {{name}}
    </h2>
    </div>
  {{/each}}
  {{#if this.isSigned}}
  {{else}}
    <button class="btn btn-success sign-course">Sign in for this course</button>
  {{/if}}
</div>
